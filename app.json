{
  "expo": {
    "name": "coudou",
    "slug": "coudou",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/profile.png",
    "splash": {
      "image": "./assets/default.png",
      "resizeMode": "contain",
      "backgroundColor": "#ecec1c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/profile.png",
        "backgroundColor": "#ecec1c"
      },
      "package": "io.expo.client"
    },
    "web": {
      "favicon": "./assets/default.png"
    }
  }
}
